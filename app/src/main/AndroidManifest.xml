<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.android.sdk13.frame_test">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dfyc"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/dfyc"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity
            android:name=".OkHttp.OKHttpActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".OkHttp.OkGoActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".XUtils3.XUtilsActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".XUtils3.annotation.XUtils3Fragment" />
        <activity android:name=".Json.GsonActivity" />
        <activity android:name=".Json.FastJsonActivity"></activity>
    </application>

</manifest>